    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.14.11",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.0.4",
    "@vitest/ui": "2.0.4",
    "dotenv": "16.4.5",
    "npm-run-all": "4.1.5",
